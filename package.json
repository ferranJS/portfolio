{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prettify": "prettier --write .",
    "prepare": "husky install",
    "set-precommit": "npx husky add .husky/pre-commit \"npx lint-staged --concurrent false\""
  },
  "dependencies": {
    "@astrojs/tailwind": "^4.0.0",
    "astro": "^2.10.7",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0"
  },
  "lint-staged": {
    "src/**/*": [
      "prettier . --write"
    ]
  }
}
